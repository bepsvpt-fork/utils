{
  "name": "laragraph/laravel-graphql-utils",
  "type": "library",
  "description": "Utilities for using GraphQL with Laravel",
  "homepage": "https://github.com/laragraph/laravel-graphql-utils",
  "license": "MIT",
  "authors": [
    {
      "name": "Benedikt Franke",
      "email": "benedikt@franke.tech"
    }
  ],
  "require": {
    "php": "^7.3",
    "illuminate/contracts": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0",
    "illuminate/http": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0",
    "thecodingmachine/safe": "^0.1.16",
    "webonyx/graphql-php": "^0.13.2 || ^v14.0.0"
  },
  "require-dev": {
    "infection/infection": "~0.13.6",
    "jangregor/phpstan-prophecy": "~0.4.2",
    "laravel/lumen-framework": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0",
    "localheinz/composer-normalize": "^1.3.1",
    "orchestra/database": "3.5.* || 3.6.* || 3.7.* || 3.8.* || 3.9.* || ^4.0",
    "orchestra/testbench": "3.5.* || 3.6.* || 3.7.* || 3.8.* || 3.9.* || ^4.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "~0.11.15",
    "phpstan/phpstan-deprecation-rules": "~0.11.2",
    "phpstan/phpstan-strict-rules": "~0.11.1",
    "phpunit/phpunit": "^8.3.4",
    "symfony/var-dumper": "^4.3",
    "thecodingmachine/phpstan-safe-rule": "^0.1.4"
  },
  "config": {
    "platform": {
      "php": "7.3.9"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Laragraph\\LaravelGraphQLUtils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Laragraph\\LaravelGraphQLUtils\\Tests\\": "tests/"
    },
    "files": [
      "vendor/symfony/var-dumper/Resources/functions/dump.php"
    ]
  },
  "support": {
    "issues": "https://github.com/laragraph/laravel-graphql-utils/issues",
    "source": "https://github.com/laragraph/laravel-graphql-utils"
  }
}
